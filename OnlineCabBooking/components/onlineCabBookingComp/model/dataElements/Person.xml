<dataElement name="Person">
  <packageName>cabBookingCore</packageName>
  <dataElementType name="Primary"/>
  <fields>
    <field name="username">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
    <field name="password">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>false</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
    <field name="email">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
    <field name="mobile">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
    <field name="address">
      <fieldType>LINK_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <linkField>
        <targetElement component="onlineCabBookingComp" name="Address"/>
        <linkFieldType name="Ln01"/>
      </linkField>
    </field>
  </fields>
  <finders>
    <finder name="findByEmailEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="">
          <operator>Eq</operator>
          <field name="email"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findByAddressEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="">
          <operator>Eq</operator>
          <field name="address"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findByMobileEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="">
          <operator>Eq</operator>
          <field name="mobile"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findByUsernameEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="">
          <operator>Eq</operator>
          <field name="username"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findAllPerson"/>
  </finders>
</dataElement>
