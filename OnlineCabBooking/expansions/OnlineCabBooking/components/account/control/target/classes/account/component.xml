      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='component-getProjection-json' class='net.democritus.component.action.ComponentProjector' method='execute'>
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

