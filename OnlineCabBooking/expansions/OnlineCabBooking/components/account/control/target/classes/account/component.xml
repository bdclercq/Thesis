      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='component-search-json' class='net.democritus.component.action.ComponentFindAction' method='execute'>
      <interceptor-ref name="jsonFinderStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='component-perform-json' class='net.democritus.component.action.ComponentPerformer' method='execute'>
