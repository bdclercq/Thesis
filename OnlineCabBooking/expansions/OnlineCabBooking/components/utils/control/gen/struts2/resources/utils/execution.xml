<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<!-- expanded with nsx-expanders:5.12.1, expansionResource net.democritus:Expanders:5.12.1 -->
<struts>

  <package name='utils-execution' namespace='/utils' extends='nsx-default'>

    <global-results>
      <result name='noaccess'>/jsp/noaccess.jsp</result>
    </global-results>

    <!-- @anchor:actions:start -->
    <!-- @anchor:actions:end -->

    <!-- Add actions here -->

    <!-- anchor:custom-actions:start -->
    <!-- anchor:custom-actions:end -->
  </package>

  <package name='utils-execution-json' namespace='/utils' extends='nsx-json-default'>

    <action name='execution-detail-json' class='net.democritus.sys.action.ExecutionDetailer' method='execute'>
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='execution-delete-json' class='net.democritus.sys.action.ExecutionDeleter' method='execute'>
      <interceptor-ref name="jsonCommandStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='execution-confirm-json' class='net.democritus.sys.action.ExecutionEnterer' method='execute'>
      <interceptor-ref name="jsonEntererStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='execution-saveDetails-json' class='net.democritus.sys.action.ExecutionEnterer' method='execute'>
      <interceptor-ref name="jsonEntererStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='execution-search-json' class='net.democritus.sys.action.ExecutionFindAction' method='execute'>
      <interceptor-ref name="jsonFinderStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='execution-perform-json' class='net.democritus.sys.action.ExecutionPerformer' method='execute'>
      <interceptor-ref name="jsonCommandStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='execution-getProjection-json' class='net.democritus.sys.action.ExecutionProjector' method='execute'>
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <!-- anchor:io-actions:start -->
    <action name='execution-upload-json' class='net.democritus.sys.action.ExecutionImporterAction' method='execute'>
        <result name='success' type='json'>
        <param name='noCache'>true</param>
        <param name='root'>jsonResult</param>
      </result>
    </action>
    <!-- anchor:io-actions:end -->

    <!-- @anchor:json-actions:start -->
    <!-- @anchor:json-actions:end -->

    <!-- Add JSON actions here -->

    <!-- anchor:custom-json-actions:start -->
    <!-- anchor:custom-json-actions:end -->
  </package>

  <package name='utils-execution-knockout-html' namespace='/utils/execution' extends='nsx-default'>
    <action name='data'>
      <result>/jsp/ps2/page.jsp?pageModel=utils/execution/execution-io-page-model</result>
    </action>
    <action name='all'>
      <result>/jsp/ps2/page.jsp?pageModel=utils/execution/execution-page-model</result>
    </action>

    <!-- @anchor:ko-html-pages:start -->
    <!-- @anchor:ko-html-pages:end -->
  </package>

  <!-- @anchor:packages:start -->
  <!-- @anchor:packages:end -->
  <!-- anchor:custom-packages:start -->
  <!-- anchor:custom-packages:end -->

</struts>
