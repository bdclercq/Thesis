    <action name='thumbnail-detail-json' class='net.democritus.gui.action.ThumbnailDetailer' method='execute'>
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='thumbnail-delete-json' class='net.democritus.gui.action.ThumbnailDeleter' method='execute'>
      <interceptor-ref name="jsonCommandStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='thumbnail-confirm-json' class='net.democritus.gui.action.ThumbnailEnterer' method='execute'>
      <interceptor-ref name="jsonEntererStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='thumbnail-saveDetails-json' class='net.democritus.gui.action.ThumbnailEnterer' method='execute'>
