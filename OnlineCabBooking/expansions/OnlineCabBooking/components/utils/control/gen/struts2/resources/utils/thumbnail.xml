      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='thumbnail-search-json' class='net.democritus.gui.action.ThumbnailFindAction' method='execute'>
      <interceptor-ref name="jsonFinderStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
      <result name='input' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='thumbnail-perform-json' class='net.democritus.gui.action.ThumbnailPerformer' method='execute'>
      <interceptor-ref name="jsonCommandStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

    <action name='thumbnail-getProjection-json' class='net.democritus.gui.action.ThumbnailProjector' method='execute'>
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
    </action>

