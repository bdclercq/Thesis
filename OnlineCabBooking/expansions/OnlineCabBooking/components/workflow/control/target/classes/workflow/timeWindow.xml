    <action name='timeWindow-perform-json' class='net.democritus.wfe.action.TimeWindowPerformer' method='execute'>
      <interceptor-ref name="jsonCommandStack" />
      <result name='success' type='json'>
        <param name='root'>jsonResult</param>
      </result>
