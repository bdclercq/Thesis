<dataElement name="Portal">
  <packageName>net.democritus.usr</packageName>
  <dataElementType name="Primary"/>
  <fields>
    <field name="name">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
      <options>
        <isNameField/>
        <isShowInfo/>
      </options>
    </field>
    <field name="version">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
      <options>
        <isShowInfo/>
      </options>
    </field>
    <field name="description">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>false</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
  </fields>
  <finders>
    <finder name="findByNameEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="Eq:name">
          <operator>Eq</operator>
          <field name="name"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findByNameEq_VersionEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="Eq:name">
          <operator>Eq</operator>
          <field name="name"/>
        </fieldOperatorPair>
        <fieldOperatorPair name="Eq:version">
          <operator>Eq</operator>
          <field name="version"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findByVersionEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="Eq:version">
          <operator>Eq</operator>
          <field name="version"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
    <finder name="findAllPortals"/>
  </finders>
</dataElement>
