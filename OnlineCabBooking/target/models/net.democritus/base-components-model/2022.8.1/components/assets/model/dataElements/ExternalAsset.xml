<dataElement name="ExternalAsset">
  <packageName>net.democritus.assets</packageName>
  <dataElementType name="Directory"/>
  <fields>
    <field name="name">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
    <field name="uri">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="StringLink"/>
      </valueField>
    </field>
    <field name="byteSize">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="Long"/>
      </valueField>
    </field>
    <field name="contentType">
      <fieldType>VALUE_FIELD</fieldType>
      <isInfoField>true</isInfoField>
      <isListField>false</isListField>
      <valueField>
        <valueFieldType component="" name="String"/>
      </valueField>
    </field>
  </fields>
  <finders>
    <finder name="findAllExternalAssets"/>
    <finder name="findByNameEq">
      <fieldOperatorPairs>
        <fieldOperatorPair name="Eq:name">
          <operator>Eq</operator>
          <field name="name"/>
        </fieldOperatorPair>
      </fieldOperatorPairs>
    </finder>
  </finders>
</dataElement>
