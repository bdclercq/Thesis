
editField(componentName, elementName, fieldName) ::= <<
<input type='text' data-bind='value: detailModel.$fieldName$.value, enable: isFieldEnabled("$elementName$.$fieldName$", editMode)'></input>
>>

displayField(componentName, elementName, fieldName) ::= <<
<img data-bind='attr: { src: $fieldName$.value }' />
>>
